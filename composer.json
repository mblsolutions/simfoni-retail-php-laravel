{
    "name": "mblsolutions/simfoniretail-php-laravel",
    "description": "Official Simfoni Retail interface for Laravel PHP applications.",
    "keywords": ["simfoniretail", "laravel"],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "MBL Solutions Ltd.",
            "email": "support@mblsolutions.co.uk"
        }
    ],
    "require": {
        "ext-json": "*",
        "illuminate/routing": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/support": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/session": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/pagination": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "mblsolutions/simfoni-retail-php": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "MBLSolutions\\SimfoniRetailLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MBLSolutions\\SimfoniRetailLaravel\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "MBLSolutions\\SimfoniRetailLaravel\\SimfoniRetailServiceProvider"
            ],
            "aliases": {
                "SimfoniRetailAuth": "\\MBLSolutions\\SimfoniRetailLaravel\\SimfoniRetailAuthFacade"
            }
        }
    }
}
